package net.taehyeon.kdtcom.dao;

import java.util.List;

import org.apache.ibatis.annotations.Mapper;

import net.taehyeon.kdtcom.model.MemberDto;

@Mapper
public interface MemberMapper {
	
	MemberDto findById(int id);

	List<MemberDto> findList();

	int insertMember(MemberDto mem);

	int updateMember(MemberDto mem);

	int deleteMember(int num);
}
